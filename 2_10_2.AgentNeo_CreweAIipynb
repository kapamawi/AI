        "\n",
        "crew = Crew(\n",
        "    agents=[content_creator, social_media_manager, analytics_expert],\n",
        "    tasks=[task1, task2, task3],\n",
        "    process=Process.sequential,\n",
        "    verbose=True,\n",
        ")\n",
        "result = crew.kickoff()\n",
        "\n",
        "print(result)\n",
        "tracer.stop()"
